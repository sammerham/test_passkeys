datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String   @id @default(cuid())
  email     String   @unique
  passKeys  PassKey[]
}

model PassKey {
  id                  String  @id @default(cuid())
  credentialID        String
  publicKey           String
  counter             Int
  transports          String[]
  deviceType          String
  backedUp            Boolean
  userId              String
  user                User     @relation(fields: [userId], references: [id])
}
